@page "/proccessedorders"

@inject IViewProcessedOrdersUseCase viewProcessedOrdersUseCase
@attribute [Authorize]
<h3>Processed Order</h3>
<br/>
<OrderListComponent Orders="_orders"></OrderListComponent>
@code {
    private IEnumerable<Order> _orders;

    protected override void OnInitialized() {
        _orders = viewProcessedOrdersUseCase.Execute();
    }
}
